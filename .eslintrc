{
  "extends": ["@pay-com/eslint-config-react"],
  "rules": {
    "no-throw-literal": "off",
    "no-unused-vars": "off",
    "consistent-return": "off",
    "no-undef": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": ["./tsconfig.json"]
      }
    }
  }
}
